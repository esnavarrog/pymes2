<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>View</th>
      <th>Body</th>
      <th>Img</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Address</th>
      <th>Facebook</th>
      <th>Twitter</th>
      <th>Articles</th>
      <th>Info</th>
      <th>Horaa</th>
      <th>Mina</th>
      <th>Horac</th>
      <th>Minc</th>
      <th>Tiempoespera</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.title %></td>
        <td><%= product.view %></td>
        <td><%= product.body %></td>
        <td><%= product.img %></td>
        <td><%= product.phone %></td>
        <td><%= product.email %></td>
        <td><%= product.address %></td>
        <td><%= product.facebook %></td>
        <td><%= product.twitter %></td>
        <td><%= product.articles_id %></td>
        <td><%= product.info %></td>
        <td><%= product.horaA %></td>
        <td><%= product.minA %></td>
        <td><%= product.horaC %></td>
        <td><%= product.minC %></td>
        <td><%= product.tiempoEspera %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
